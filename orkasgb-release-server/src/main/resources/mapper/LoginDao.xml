<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.orkasgb.orkasgbserver.dao.login.LoginDao">
    <select id="loginValidate" parameterType="com.orkasgb.orkasgbserver.entity.OperatorEntity"
            resultType="com.orkasgb.orkasgbserver.entity.OperatorEntity">
        select * from operator where operatorLoginId = #{operatorLoginId}
    </select>
</mapper>